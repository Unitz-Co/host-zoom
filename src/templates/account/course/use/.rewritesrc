{
  "rewrites": [
    {
      "originPath": "/account/course/use-template",
      "render": "renderAccountPage",
      "source": "/*/course/use-template",
      "function": "seo-tool-account-course-useTemplate"
    },
    {
      "originPath": "/account/course/use-template",
      "render": "renderAccountPageData",
      "source": "/page-data/*/course/use-template/page-data.json",
      "function": "seo-tool-account-course-useTemplatePageData"
    }
  ],
  "locales": ["en", "de"]
}