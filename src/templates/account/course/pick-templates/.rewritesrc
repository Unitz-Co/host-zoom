{
  "rewrites": [
    {
      "originPath": "/account/course/pick-templates",
      "render": "renderAccountPage",
      "source": "/*/course/pick-templates",
      "function": "seo-tool-account-course-pickTemplates"
    },
    {
      "originPath": "/account/course/pick-templates",
      "render": "renderAccountPageData",
      "source": "/page-data/*/course/pick-templates/page-data.json",
      "function": "seo-tool-account-course-pickTemplatesPageData"
    }
  ],
  "locales": ["en", "de"]
}