{
  "rewrites": [
    {
      "originPath": "/account/student",
      "render": "renderAccountPage",
      "source": "/*/student",
      "function": "seo-tool-account-student"
    },
    {
      "originPath": "/account/student",
      "render": "renderAccountPageData",
      "source": "/page-data/*/student/page-data.json",
      "function": "seo-tool-account-studentPageData"
    }
  ],
  "locales": ["en", "de"]
}